<project default="compile">

    <presetdef name="javac">
<javac includeantruntime="false" />
    </presetdef>

    <target name="clean">
        <delete dir="bin"/>
    </target>

    <target name="compile">
        <mkdir dir="bin"/>
        <javac srcdir="src" destdir="bin"/>
    </target>
    
</project>
